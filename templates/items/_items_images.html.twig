<div class="mb-3">
    <div class="row g-2">

        <div class="card">
            <div class="card-body">
                <h3 class="card-title">{{ "Upload images"|trans }}</h3>
                <div class="dropzone" id="dropzone-multiple">
                    <div class="fallback">
                        <input name="images[]" type="file" multiple/>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>

